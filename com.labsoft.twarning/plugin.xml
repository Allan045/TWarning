<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Threshold"
            id="com.labsoft.twarning">
      </category>
      <view
            name="Threshold Warning View"
            inject="true"
            icon="icons/sample.png"
            category="com.labsoft.twarning"
            class="com.labsoft.twarning.views.TWarningView"
            id="com.labsoft.twarning.views.TWarningView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="com.labsoft.twarning.views.TWarningView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
